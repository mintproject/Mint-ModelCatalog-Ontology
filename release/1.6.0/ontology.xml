    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/okn/o/sdm#hasCoordinateSystem -->


    <owl:DatatypeProperty rdf:about="https://w3id.org/okn/o/sdm#hasCoordinateSystem">
